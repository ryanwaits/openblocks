{
  "name": "@waits/lively-storage",
  "version": "0.0.1",
  "type": "module",
  "description": "CRDT storage types for Lively real-time collaboration",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "bunup src/index.ts --format esm,cjs --dts"
  },
  "dependencies": {
    "@waits/lively-types": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5"
  }
}
